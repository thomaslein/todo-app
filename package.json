  "version": "0.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "postinstall": "npm run build",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint assets/client/scripts",
    "clean": "rimraf web/assets",
    "dev": "cross-env NODE_ENV=development node index",
    "prebuild": "npm run clean -s",
    "build": "cross-env NODE_ENV=build webpack -p --colors --progress",
    "prebuild:dev": "npm run clean -s",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --progress",
    "preproduction": "npm run clean -s",
    "production": "cross-env NODE_ENV=production webpack -p --colors --progress",
    "deploy": "node deploy",
    "postdeploy": "npm-run-all lint test production",
    "serve": "nodemon server/index.js"
  },
  "jest": {
    "watchman": false,
    "verbose": true,
    "setupFiles": [
      "<rootDir>/jest/setupJest.js"
    ],
    "transform": {
      ".*": "<rootDir>/jest/preprocessor.js"
    },
    "roots": [
      "<rootDir>/src/scripts"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/jest/styleStub.js",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/fileStub.js"
    }
  },
